{
 "name": "boundless-api-client",
 "version": "0.0.10",
 "description": "The official JavaScript + TypeScript client library for Boundless Commerce",
 "keywords": [
  "boundless",
  "boundless-commerce",
  "cms",
  "headless",
  "ecommerce",
  "api",
  "checkout",
  "typescript",
  "cms",
  "cart"
 ],
 "repository": "https://github.com/kirill-zhirnov/boundless-api-client",
 "author": "Kirill Zhirnov <ekvixgroup@gmail.com>",
 "license": "MIT",
 "scripts": {
  "build": "rm -rf ./dist && npx rollup --config rollup.prod.js",
  "lint": "npx eslint --ext .ts ./src/"
 },
 "private": false,
 "main": "./dist/index.js",
 "types": "./dist/index.d.ts",
 "files": [
  "dist"
 ],
 "exports": {
  ".": "./dist/index.js",
