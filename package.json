  "boundless",
  "boundless-commerce",
  "cms",
  "headless",
  "ecommerce",
  "api",
  "checkout",
  "typescript",
  "cms",
  "cart"
 ],
 "repository": "https://github.com/kirill-zhirnov/boundless-api-client",
 "author": "Kirill Zhirnov <ekvixgroup@gmail.com>",
 "license": "MIT",
 "scripts": {
  "build": "rm -rf ./dist && npx rollup --config rollup.prod.mjs",
  "lint": "npx eslint --ext .ts ./src/"
 },
 "private": false,
 "main": "./dist/index.js",
 "types": "./dist/index.d.ts",
 "files": [
  "dist"
 ],
 "exports": {
  ".": "./dist/index.js",
